{
	"name": "termost",
	"description": "Get the most of your terminal",
	"version": "1.4.0",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"sideEffects": false,
	"files": [
		"dist"
	],
	"exports": {
		"source": "./src/index.ts",
		"types": "./dist/index.d.ts",
		"require": "./dist/index.cjs",
		"import": "./dist/index.mjs",
		"default": "./dist/index.mjs"
	},
	"keywords": [
		"cli",
		"terminal",
		"args",
		"argument",
		"option",
		"command",
		"task",
		"question",
		"message"
	],
	"license": "MIT",
	"author": "Ayoub Adib <adbayb@gmail.com> (https://twitter.com/adbayb)",
	"bugs": "https://github.com/adbayb/termost/issues",
	"homepage": "https://github.com/adbayb/termost/tree/main/termost#readme",
	"repository": {
		"type": "git",
		"url": "git@github.com:adbayb/termost.git",
		"directory": "termost"
	},
	"scripts": {
		"prepublishOnly": "pnpm build",
		"start": "pnpm watch",
		"build": "quickbundle build",
		"watch": "quickbundle watch",
		"test": "vitest --test-timeout=20000"
	},
	"dependencies": {
		"enquirer": "^2.4.1",
		"listr2": "^8.2.5",
		"picocolors": "^1.1.1"
	},
	"devDependencies": {
		"@types/node": "22.13.5",
		"quickbundle": "2.8.0",
		"vitest": "3.0.5"
	}
}
